{
  "version": "1.0.0",
  "description": "Dynamic Model Configuration for Consensus Vault Orchestrator",
  "defaultProvider": "openai",
  "models": {
    "deepseek": {
      "id": "deepseek",
      "name": "Momentum Hunter",
      "role": "Technical Analysis & Trend Detection",
      "baseUrl": "https://api.deepseek.com/v1",
      "apiKeyEnv": "DEEPSEEK_API_KEY",
      "model": "deepseek-chat",
      "provider": "openai",
      "timeout": 30000,
      "priority": "primary",
      "costPerToken": 0.00014,
      "maxTokens": 16000,
      "enabled": true,
      "systemPrompt": "You are the Momentum Hunter, an expert technical analyst specializing in cryptocurrency markets. Analyze price action, indicators, and trends to provide trading signals."
    },
    "kimi": {
      "id": "kimi",
      "name": "Whale Watcher",
      "role": "Large Holder Movements & Accumulation Patterns",
      "baseUrl": "https://api.kimi.com/coding/v1",
      "apiKeyEnv": "KIMI_API_KEY",
      "model": "k2p5",
      "provider": "anthropic",
      "timeout": 30000,
      "priority": "primary",
      "costPerToken": 0.0003,
      "maxTokens": 32000,
      "enabled": true,
      "systemPrompt": "You are the Whale Watcher, an expert in analyzing large holder behavior and institutional movements in crypto markets."
    },
    "minimax": {
      "id": "minimax",
      "name": "Sentiment Scout",
      "role": "Social Sentiment & Community Buzz",
      "baseUrl": "https://api.minimax.io/v1",
      "apiKeyEnv": "MINIMAX_API_KEY",
      "model": "MiniMax-M2",
      "provider": "openai",
      "timeout": 30000,
      "priority": "secondary",
      "costPerToken": 0.0001,
      "maxTokens": 32000,
      "enabled": true,
      "systemPrompt": "You are the Sentiment Scout, an expert in analyzing social sentiment and community dynamics in crypto markets."
    },
    "glm": {
      "id": "glm",
      "name": "On-Chain Oracle",
      "role": "On-Chain Metrics & TVL Analysis",
      "baseUrl": "https://api.z.ai/api/anthropic/v1",
      "apiKeyEnv": "GLM_API_KEY",
      "model": "glm-4.6",
      "provider": "anthropic",
      "timeout": 30000,
      "priority": "secondary",
      "costPerToken": 0.0002,
      "maxTokens": 128000,
      "enabled": true,
      "systemPrompt": "You are the On-Chain Oracle, an expert in blockchain analytics and on-chain metrics for crypto markets."
    },
    "gemini": {
      "id": "gemini",
      "name": "Risk Manager",
      "role": "Risk Assessment & Portfolio Exposure",
      "baseUrl": "https://generativelanguage.googleapis.com/v1beta",
      "apiKeyEnv": "GEMINI_API_KEY",
      "model": "gemini-2.5-flash",
      "provider": "google",
      "timeout": 30000,
      "priority": "fallback",
      "costPerToken": 0.000075,
      "maxTokens": 1000000,
      "enabled": true,
      "systemPrompt": "You are the Risk Manager, an expert in risk assessment and portfolio management for crypto markets."
    },
    "claude-opus": {
      "id": "claude-opus",
      "name": "Claude Opus (Premium)",
      "role": "Advanced Reasoning & Analysis",
      "baseUrl": "https://api.anthropic.com/v1",
      "apiKeyEnv": "ANTHROPIC_API_KEY",
      "model": "claude-3-opus-20240229",
      "provider": "anthropic",
      "timeout": 60000,
      "priority": "emergency",
      "costPerToken": 0.015,
      "maxTokens": 200000,
      "enabled": false,
      "systemPrompt": "You are an advanced AI analyst with exceptional reasoning capabilities. Provide deep, nuanced analysis for complex market scenarios."
    },
    "gpt4": {
      "id": "gpt4",
      "name": "GPT-4 Turbo",
      "role": "General Purpose Analysis",
      "baseUrl": "https://api.openai.com/v1",
      "apiKeyEnv": "OPENAI_API_KEY",
      "model": "gpt-4-turbo-preview",
      "provider": "openai",
      "timeout": 45000,
      "priority": "emergency",
      "costPerToken": 0.01,
      "maxTokens": 128000,
      "enabled": false,
      "systemPrompt": "You are a versatile AI analyst capable of handling a wide range of analytical tasks. Provide balanced, comprehensive analysis."
    }
  },
  "fallbackChains": {
    "deepseek": ["minimax", "glm", "kimi", "gemini"],
    "kimi": ["glm", "deepseek", "minimax", "gemini"],
    "minimax": ["deepseek", "glm", "kimi", "gemini"],
    "glm": ["kimi", "deepseek", "minimax", "gemini"],
    "gemini": ["minimax", "deepseek", "glm", "kimi"],
    "claude-opus": ["glm", "kimi", "deepseek", "gemini"],
    "gpt4": ["deepseek", "minimax", "glm", "kimi"]
  }
}
